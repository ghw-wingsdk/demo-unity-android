<?xml version="1.0" encoding="utf-8"?>
<animation-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:oneshot="false"
    android:variablePadding="false">
    <item android:duration="100">
        <rotate android:drawable="@drawable/ic_progress_inverse"
            android:pivotX="50%"
            android:pivotY="50%"
            android:fromDegrees="0"
            android:toDegrees="0"
            android:duration="0"/>
    </item>
    <item android:duration="100">
        <rotate android:drawable="@drawable/ic_progress_inverse"
            android:pivotX="50%"
            android:pivotY="50%"
            android:fromDegrees="30"
            android:toDegrees="30"
            android:duration="0"/>
    </item>
    <item android:duration="100">
        <rotate android:drawable="@drawable/ic_progress_inverse"
            android:pivotX="50%"
            android:pivotY="50%"
            android:fromDegrees="60"
            android:toDegrees="60"
            android:duration="0"/>
    </item>
    <item android:duration="100">
        <rotate android:drawable="@drawable/ic_progress_inverse"
            android:pivotX="50%"
            android:pivotY="50%"
            android:fromDegrees="90"
            android:toDegrees="90"
            android:duration="0"/>
    </item>
    <item android:duration="100">
        <rotate android:drawable="@drawable/ic_progress_inverse"
            android:pivotX="50%"
            android:pivotY="50%"
            android:fromDegrees="120"
            android:toDegrees="120"
            android:duration="0"/>
    </item>
    <item android:duration="100">
        <rotate android:drawable="@drawable/ic_progress_inverse"
            android:pivotX="50%"
            android:pivotY="50%"
            android:fromDegrees="150"
            android:toDegrees="150"
            android:duration="0"/>
    </item>
    <item android:duration="100">
        <rotate android:drawable="@drawable/ic_progress_inverse"
            android:pivotX="50%"
            android:pivotY="50%"
            android:fromDegrees="180"
            android:toDegrees="180"
            android:duration="0"/>
    </item>
    <item android:duration="100">
        <rotate android:drawable="@drawable/ic_progress_inverse"
            android:pivotX="50%"
            android:pivotY="50%"
            android:fromDegrees="210"
            android:toDegrees="210"
            android:duration="0"/>
    </item>
    <item android:duration="100">
        <rotate android:drawable="@drawable/ic_progress_inverse"
            android:pivotX="50%"
            android:pivotY="50%"
            android:fromDegrees="240"
            android:toDegrees="240"
            android:duration="0"/>
    </item>
    <item android:duration="100">
        <rotate android:drawable="@drawable/ic_progress_inverse"
            android:pivotX="50%"
            android:pivotY="50%"
            android:fromDegrees="270"
            android:toDegrees="270"
            android:duration="0"/>
    </item>
    <item android:duration="100">
        <rotate android:drawable="@drawable/ic_progress_inverse"
            android:pivotX="50%"
            android:pivotY="50%"
            android:fromDegrees="300"
            android:toDegrees="300"
            android:duration="0"/>
    </item>
    <item android:duration="100">
        <rotate android:drawable="@drawable/ic_progress_inverse"
            android:pivotX="50%"
            android:pivotY="50%"
            android:fromDegrees="330"
            android:toDegrees="330"
            android:duration="0"/>
    </item>
</animation-list>